node{
   stage('SCM Checkout'){
     git 'https://github.com/Smaheshwar85/Devops_basic_step.git'
   }
 stage('Maven Build'){
        steps{
         sh "mvn clean package"
        }
   stage('Deploy to Tomcat'){
      
      sshagent(['tomcat-dev']) {
         sh 'scp -o StrictHostKeyChecking=no target/*.war ubuntu@172.31.94.14:/opt/xtomcat/webapps/'
      }
   }
}
